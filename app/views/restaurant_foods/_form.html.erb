
<%= form_with(model: [@restaurant, @restaurant_food], local:true ) do |f| %>

  <div >
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <div class="form-group">
    <%= f.label :price %>
    <%= f.text_field :price %>
  </div>

  <div >
    <%= f.label :food_picture %>
    <%= f.file_field :food_picture %>
  </div>


  <%= f.submit  %>

  <% end %>



