<body class="session-bg">

<section class="login">
<%= form_for :session, url: sessions_path do |f| %>
  <div >
    <%= f.label :email %>
    <%= f.email_field :email, class: "form-control" %>
  </div>
  <p>

  <div >
    <%= f.label :password %>
    <%= f.password_field :password, class: "form-control" %>
  </div>
  <p>
  <%= f.submit "Sign In", class: "btn btn-outline-primary" %>
  
<% end %>
</section>

</body>




